{
  "name": "rbr-setups",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --ssr",
    "preview": "$npm_execpath run build && wrangler dev dist-server/index.js",
    "deploy": "$npm_execpath run build && wrangler deploy dist-server/index.js",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "drizzle-kit generate",
    "db:up": "drizzle-kit up",
    "test": "vitest"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@hono/auth-js": "^1.0.15",
    "@libsql/client": "^0.15.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "hono": "^4.7.9"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^0.1.15",
    "@cloudflare/vitest-pool-workers": "^0.8.27",
    "@hono/vite-build": "^1.5.0",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.19.4",
    "vite": "^6.1.1",
    "vite-plugin-ssr-hot-reload": "^0.4.1",
    "vitest": "~3.1.0",
    "wrangler": "^4.4.0"
  }
}
